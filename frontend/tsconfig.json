{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmitOnError": false,
    "noImplicitAny": false,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "target": "ES6"
  },
  "include": ["next-env.d.ts"],
  "exclude": ["node_modules"]
}
